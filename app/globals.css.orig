/* Container for the form */
.signup-container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 100%;
  max-width: 400px;
  margin: 50px auto;
  padding: 20px;
  border: 1px solid #e0e0e0;
  border-radius: 10px;
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
  background-color: #f9f9f9;
}

/* Form styling */
.signup-container form {
  width: 100%;
}

/* Form heading */
.signup-container h2 {
  margin-bottom: 20px;
  font-size: 24px;
  color: #333;
  text-align: center;
}

/* Label styling */
.signup-container label {
  display: block;
  margin-bottom: 10px;
  font-size: 16px;
  color: #333;
}

/* Input styling */
.signup-container input {
  width: 100%;
  padding: 10px;
  margin-bottom: 15px;
  border: 1px solid #ccc;
  border-radius: 5px;
  box-sizing: border-box;
  transition: border-color 0.3s;
}

/* Change border color on focus */
.signup-container input:focus {
  border-color: #007bff;
  outline: none;
}

/* Button styling */
.signup-container button {
  width: 100%;
  padding: 10px;
  border: none;
  border-radius: 5px;
  background-color: #007bff;
  color: white;
  font-size: 16px;
  cursor: pointer;
  transition: background-color 0.3s;
}

/* Change background color on hover */
.signup-container button:hover {
  background-color: #0056b3;
}

/* Adding some margin to break lines */
.signup-container br {
  display: none;
}
